@model TripApplication.Models.PlaceDto

@{
    ViewBag.Title = "DeleteConfirm";
}

<h2>Are you sure you want to delete this Place?</h2>
<a href="/Place/List">Cancel</a>
<h2>@Model.PlaceName</h2>
<div>Description: @Model.PlaceDescription </div>
<div>City: @Model.PlaceCity</div>
<form action="/Place/Delete/@Model.PlaceId" method="POST">
    <input type="submit" value="Confirm Delete" />

</form>
